# Copy this file to .env and fill in your values
# .env is git-ignored for security

# Timezone
TZ=America/Los_Angeles

# User/Group IDs (run 'id' command on server to find yours)
PUID=1000
PGID=1000

# Paths on the host server
DATA_DIR=/mnt/media
CONFIGS_DIR=/srv/configs

# OpenVPN credentials from ProtonVPN (Account -> OpenVPN)
OPENVPN_USER=
OPENVPN_PASSWORD=